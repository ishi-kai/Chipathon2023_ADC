simulator=ngspice
only_toplevel=false 
value="
VDD CLK 0 pulse (0 3.3 0 40p 40p 62.5n 125n) dc 0v
.control
save all
let VSTART = 0
let VSTOP  = 10m
let VDELTA = 0.5m
let v_act   = 0
let i = 0;
while v_act < VSTOP
	tran 0.1n 40n
	alter VIN = v_act
	let v_act = v_act + VDELTA
	write comparator_tb.raw
	set appendwrite
end 
.endc
"}
